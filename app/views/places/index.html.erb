<% @places.each do |place| %>
<p><%= place.name %></p>
<p><%= place.message %></p>
<img src="<%= "/place_images/#{place.image_name}" %>" width="150" height="150">
<%= link_to'編集', edit_place_path(place.id) %>
<%= link_to'詳細', place_path(place.id) %>
<%= link_to "削除", place_path(place.id), method: :delete, data: {confirm: "削除しますか？"} %>
<% end %>
